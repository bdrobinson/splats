parser: babel-eslint
env:
  browser: true
  node: true
extends:
  - standard
  - standard-react
rules:
  no-unused-vars: 'warn'
  comma-dangle:
    - 'error'
    - 'always-multiline'
  indent:
    - 'error'
    - 4
  react/jsx-indent:
    - 'error'
    - 4
  react/jsx-indent-props:
    - 'error'
    - 4
  react/prop-types: 0
